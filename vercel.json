{
  "functions": {
    "api/get.ts": {
      "includeFiles": "themes/**"
    }
  },
  "rewrites": [
    {
      "source": "/get/@:key",
      "destination": "/api/get.ts"
    },
    {
      "source": "/record/@:key",
      "destination": "/api/record.ts"
    }
  ],
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        }
      ]
    }
  ]
}
